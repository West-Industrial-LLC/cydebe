# Frontend Application Environment Variables
# Copy this file to .env.local and fill in your values

# Backend URLs
NEXT_PUBLIC_API_BASE_URL=http://localhost:3004
NEXT_PUBLIC_LOCAL_NETWORK_URL=http://localhost:3002
NEXT_PUBLIC_LOCAL_DEVICE_URL=http://localhost:3003

# External Services
OPENAI_API_KEY=your_openai_api_key_here
JITSI_DOMAIN=meet.jit.si
JITSI_APP_ID=

# Application Settings
NEXT_PUBLIC_APP_NAME=Cydebe
NEXT_PUBLIC_APP_VERSION=1.0.0
NEXT_PUBLIC_DEFAULT_LANGUAGE=en

# Analytics
NEXT_PUBLIC_ANALYTICS_ENABLED=true
NEXT_PUBLIC_ANALYTICS_URL=

# Security
NEXT_PUBLIC_CSP_ENABLED=true
NEXT_PUBLIC_CSP_REPORT_URI=

# Development
NODE_ENV=development
NEXT_PUBLIC_DEBUG=true
